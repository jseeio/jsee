{
  "name": "@jseeio/jsee",
  "version": "0.3.0",
  "description": "JavaScript Execution Environment",
  "main": "dist/jsee.js",
  "unpkg": "dist/jsee.js",
  "jsdelivr": "dist/jsee.js",
  "private": false,
  "scripts": {
    "build-dev": "webpack --mode=development --progress --stats-children --env DEVELOPMENT",
    "build": "webpack --mode=production --progress && webpack --mode=production --progress --env RUNTIME",
    "watch": "nodemon --watch . --ignore dist,.git --ext vue,js,css,html --exec 'npm run build-dev && npm test'",
    "prepublishOnly": "npm run build && npm test",
    "test": "jest test.js --detectOpenHandles",
    "test-head": "HEADLESS=false npm test"
  },
  "author": "Anton Zemlyansky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jseeio/jsee"
  },
  "homepage": "https://jsee.org",
  "bugs": {
    "url": "https://github.com/jseeio/jsee/issues"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "bulma": "^0.9.3",
    "csv-parse": "^4.6.1",
    "element-plus": "^1.3.0-beta.1",
    "file-saver": "^2.0.2",
    "filtrex": "^2.2.3",
    "notyf": "^3.10.0",
    "vue": "^3.2.47",
    "vue-style-loader": "^4.1.3",
    "vue3-json-viewer": "^2.2.2",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.16.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "expect-puppeteer": "^8.0.5",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-puppeteer": "^8.0.6",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.22",
    "puppeteer": "^19.8.5",
    "sass-loader": "^13.2.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.7",
    "uglify-es": "^3.3.9",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "worker-loader": "^3.0.8"
  },
  "resolutions": {
    "ansi-regex": "5.0.1"
  }
}
